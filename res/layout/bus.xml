<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" style="@style/studentguide_layout">
	
	<include layout="@layout/statusbar" android:id="@+id/statusbar"/>


	<EditText android:layout_width="fill_parent" android:layout_height="100dip"
		android:layout_centerInParent="true"  android:id="@+id/et_ask_bus"
		android:hint="Skriv ditt spørsmål her" />
		
	<Button android:layout_height="wrap_content" android:layout_width="200dip" android:id="@+id/btn_ask_bus" android:text="Send" android:layout_below="@id/et_ask_bus" android:layout_centerHorizontal="true" />


</RelativeLayout>